{
  "name": "ludumpad-mapper-electron",
  "version": "0.0.0",
  "description": "Electron app for mapping keys to LudumPad input.",
  "keywords": [
    "ludumpad",
    "server",
    "controller",
    "socket.io",
    "map",
    "keys"
  ],
  "homepage": "https://github.com/dasilvacontin/ludumpad/tree/master/packages/ludumpad-mapper-electron",
  "bugs": "https://github.com/dasilvacontin/ludumpad/issues",
  "license": "LGPL-3.0",
  "author": {
    "name": "David da Silva Cont√≠n",
    "email": "dasilvacontin@gmail.com",
    "url": "http://dasilvacont.in"
  },
  "files": [
    "lib"
  ],
  "main": "lib/app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dasilvacontin/ludumpad.git"
  },
  "scripts": {
    "lint": "eslint src",
    "typecheck": "flow",
    "pretest": "npm run lint && npm run typecheck",
    "test": "mocha --compilers js:babel-register",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel-register",
    "ci": "dot-only-hunter && npm run coverage && cat ./coverage/lcov.info | coveralls",
    "build": "rsync -av --exclude=*.js src/ lib/ && babel src --out-dir lib",
    "prepublish": "npm run build",
    "start": "electron lib/app.js"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "electron-prebuilt": "^1.1.0",
    "json-beautify": "^1.0.1",
    "ludumpad-mapper": "~0.0.3",
    "qr-image": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.4",
    "babel-plugin-lodash": "^3.0.0",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "browserify": "^13.0.0",
    "coveralls": "^2.11.9",
    "dot-only-hunter": "^1.0.2",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.0",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "flow-bin": "^0.24.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "unexpected": "^10.13.0"
  }
}
